{
  "name": "primeekart-backend",
  "version": "1.0.0",
  "description": "Backend for Gamiex e-commerce platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "data:import": "node seeder",
    "data:destroy": "node seeder -d",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop gaming-store-api",
    "pm2:restart": "pm2 restart gaming-store-api",
    "pm2:reload": "pm2 reload gaming-store-api",
    "pm2:delete": "pm2 delete gaming-store-api",
    "pm2:logs": "pm2 logs gaming-store-api",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "keep-alive": "node scripts/keep-atlas-alive.js",
    "test-keep-alive": "node scripts/test-keep-alive.js",
    "setup-cronjob": "scripts/setup-cronjob.sh",
    "setup-windows-task": "scripts/setup-windows-task.bat",
    "setup-windows-task-ps": "powershell -ExecutionPolicy Bypass -File scripts/setup-windows-task.ps1"
  },
  "keywords": [
    "ecommerce",
    "express",
    "mongodb",
    "jwt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "nodemon": "^3.0.1"
  }
}
